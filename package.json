{
  "name": "td-workflow-development-kit",
  "version": "1.0.0",
  "description": "TreasureData Workflow Development Kit",
  "private": true,
  "bin": {
    "td-wdk": "./bin/main.js"
  },
  "engines": {
    "node": ">= 10.15.3",
    "npm": ">= 6.13.4"
  },
  "scripts": {
    "lint": "eslint --fix 'src/**/*.ts' --no-error-on-unmatched-pattern && eslint --fix 'test/**/*.ts' --no-error-on-unmatched-pattern",
    "test": "jest --passWithNoTests",
    "build": "npm run clean && npm run lint && npm run test && tsc",
    "install": "tsc",
    "clean": "rimraf ./dist",
    "setup": "npm link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chatwork/td-workflow-development-kit.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chatwork/td-workflow-development-kit/issues"
  },
  "homepage": "https://github.com/chatwork/td-workflow-development-kit#readme",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1"
  },
  "jest": {
    "globals": {},
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(tests?/.+(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@types/commander": "2.12.2",
    "@types/fs-extra": "8.1.0",
    "@types/yaml": "1.2.0",
    "chalk": "3.0.0",
    "commander": "4.1.1",
    "csv": "5.3.2",
    "fs-extra": "8.1.0",
    "log-symbols": "3.0.0",
    "ora": "4.0.3",
    "td-workflow-client": "github:chatwork/td-workflow-client#fed88ca5464253c8f7872fc3c91aebec9397e600",
    "typescript": "3.8.2",
    "yaml": "1.7.2"
  }
}
